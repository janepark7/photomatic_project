<body class="upload-container">

<form class="upload" enctype="multipart/form-data" method="post">

<% if(typeof error !== undefined) { %>
	<div class= "error"><%= error %></div>
	<%}%>
</br>

	<input class= "file" type="file" name="file"/>
	<textarea name="description" rows="8" cols="80" placeholder="Please Enter A Description"></textarea>

	<button id="upload-button" class="button">Upload</button>


</form>
</body>
