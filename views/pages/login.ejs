
<body>
<form class = "Form" method="post">
	<% if(typeof error !== undefined) { %>
		<div class= "error"><%= error %></div>
	<%}%>
	<div class="username-form">
		Username
	</div>
		<input class="login-username" type="text" name = "username"
		placeholder="Please Enter Your Username"></input><br>


	<div class="password-form">
		Password
	</div>
		<input class="login-password" type="password" name = "login-password"
		placeholder="Please Enter Your Password"></input><br>


	<button class="submit">Submit</button>

</form>

<script scr = "/assets/login.js"></script>
</body>
